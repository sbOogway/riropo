{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "no-console": "off",
        "no-unused-vars": "off",
        "no-debugger": "off"
    }
}